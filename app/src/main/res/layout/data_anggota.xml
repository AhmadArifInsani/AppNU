<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WhiteColor"
    tools:context=".DataAnggota">
    <RelativeLayout
        android:id="@+id/footer_data_anggota"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/footer"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:padding="3dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_back" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_toEndOf="@id/back"
            android:gravity="center">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppinssemibold"
                android:text="Data Anggota"
                android:textColor="@color/BlackColor"
                android:textSize="20dp" />
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/search"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/menu_abu_radius20"
        android:layout_marginTop="20dp"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/footer_data_anggota">

        <SearchView
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginRight="10dp"
            android:iconifiedByDefault="false"
            android:queryHint="Cari Nama" />
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/background_data_anggota"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:background="@drawable/menu_abu_radius10"
        android:orientation="vertical"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/search">

        <RelativeLayout
            android:id="@+id/profil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="20dp">

            <ImageView
                android:layout_width="95dp"
                android:layout_height="120dp"
                android:background="@drawable/foto_profil" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/nama"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="5dp"
            android:gravity="center_vertical"
            android:layout_toEndOf="@id/profil">

            <TextView
                android:id="@+id/text_nama"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical"
                android:fontFamily="@font/poppinsbold"
                android:text="Ahmad Arif Insani"
                android:textColor="@color/BlackColor" />
            <ImageView
                android:id="@+id/line"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_below="@id/text_nama"
                android:src="@drawable/line"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:layout_below="@id/line"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/text_tanggal_lahir"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="26 September 1997"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/poppinsregular"/>
                <TextView
                    android:id="@+id/text_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="arifinsani40@gmail.com"
                    android:layout_below="@id/text_tanggal_lahir"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/poppinsregular"/>
                <TextView
                    android:id="@+id/text_pimpinan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="PR Plosokerep"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:layout_below="@id/text_email"
                    android:fontFamily="@font/poppinsregular"/>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_below="@id/nama"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="10dp"
            android:gravity="center_vertical"
            android:layout_toEndOf="@id/profil"
            android:background="@drawable/menu_grey_radius10">
            <TextView
                android:id="@+id/text_nomor"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_marginLeft="7dp"
                android:textSize="15sp"
                android:text="+62 897-8768-8768"
                android:gravity="center_vertical"
                android:textColor="@color/BlackColor"
                android:fontFamily="@font/poppinsregular"/>
            <RelativeLayout
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_toEndOf="@id/text_nomor">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/logo_whatsapp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/background_data_anggota_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="20dp"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/menu_abu_radius10"
        android:orientation="vertical"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/background_data_anggota">

        <RelativeLayout
            android:id="@+id/profil_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="20dp">
            <ImageView
                android:layout_width="95dp"
                android:layout_height="120dp"
                android:background="@drawable/foto_profil"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/nama_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="5dp"
            android:gravity="center_vertical"
            android:layout_toEndOf="@id/profil_1">

            <TextView
                android:id="@+id/text_nama_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical"
                android:fontFamily="@font/poppinsbold"
                android:text="Ahmad Arif Insani"
                android:textColor="@color/BlackColor" />
            <ImageView
                android:id="@+id/line_1"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="30dp"
                android:layout_below="@id/text_nama_1"
                android:src="@drawable/line"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:layout_below="@id/line_1"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/text_tanggal_lahir_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="26 September 1997"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/poppinsregular"/>
                <TextView
                    android:id="@+id/text_email_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="arifinsani40@gmail.com"
                    android:layout_below="@id/text_tanggal_lahir_1"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/poppinsregular"/>
                <TextView
                    android:id="@+id/text_pimpinan_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:text="PR Plosokerep"
                    android:textColor="@color/BlackColor"
                    android:gravity="center_vertical"
                    android:layout_below="@id/text_email_1"
                    android:fontFamily="@font/poppinsregular"/>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_below="@id/nama_1"
            android:layout_marginStart="5dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="10dp"
            android:gravity="center_vertical"
            android:layout_toEndOf="@id/profil_1"
            android:background="@drawable/menu_grey_radius10">
            <TextView
                android:id="@+id/text_nomor_1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:textSize="15sp"
                android:layout_marginLeft="7dp"
                android:text="+62 897-8768-8768"
                android:gravity="center_vertical"
                android:textColor="@color/BlackColor"
                android:fontFamily="@font/poppinsregular"/>
            <RelativeLayout
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_toEndOf="@id/text_nomor_1">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/logo_whatsapp"
                    android:layout_centerVertical="true"
                    />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>